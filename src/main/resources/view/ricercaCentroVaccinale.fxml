<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="500.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.studenti.uninsubria.clientCV.controller.RicercaCentroVaccinaleController">
   <left>
      <AnchorPane prefHeight="500.0" prefWidth="650.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="204.0" layoutY="14.0" prefHeight="24.0" prefWidth="242.0" text="Ricerca Centro Vaccinale">
               <font>
                  <Font name="Arial Bold" size="20.0" />
               </font></Label>
            <CheckBox fx:id="checkBoxNome" layoutX="101.0" layoutY="47.0" mnemonicParsing="false" text="Ricerca per Nome: " />
            <CheckBox fx:id="checkBoxComuneETipologia" layoutX="308.0" layoutY="47.0" mnemonicParsing="false" text="Ricerca per Comune e Tipologia:" />
            <TextField fx:id="txtFieldNome" layoutX="88.0" layoutY="69.0" />
            <TextField fx:id="txtFieldComune" layoutX="257.0" layoutY="69.0" />
            <ComboBox fx:id="cBoxTipologia" layoutX="411.0" layoutY="69.0" prefWidth="150.0" />
            <Button fx:id="btnMostraRisultati" layoutX="278.0" layoutY="103.0" mnemonicParsing="false" onMouseClicked="#ricercaCentriVaccinali" text="Mostra i Risultati" />
            <ListView fx:id="listViewCentriVaccinali" layoutX="80.0" layoutY="140.0" prefHeight="325.0" prefWidth="501.0" />
            <Label fx:id="lblNomeMancante" layoutX="124.0" layoutY="103.0" text="Inserire Nome" textFill="RED" visible="false" />
            <Label fx:id="lblComuneOTipologiaMancanti" layoutX="418.0" layoutY="107.0" text="Inserire Nome o Tipologia" textFill="RED" visible="false" />
         </children>
      </AnchorPane>
   </left>
</BorderPane>
